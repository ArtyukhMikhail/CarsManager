{
	"_version": "1.12.0",
	"sap.app": {
		"id": "masterdetail.MasterList",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"Z_CARS_SRV": {
				"uri": "/IDES_SAPUI5_School/sap/opu/odata/sap/Z_CARS_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/Z_CARS_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"handleValidation": true,
		"rootView": {
			"viewName": "masterdetail.MasterList.view.App",
			"type": "XML",
			"async": true,
			"id": ""
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "masterdetail.MasterList.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "Z_CARS_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "masterdetail.MasterList.view",
				"controlId": "idSplitApp",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "Start",
				"pattern": "",
				"target": ["Start", "EmptyObject"]
			}, {
				"name": "DetailObject",
				"pattern": "car/{id}",
				"target": ["Start", "DetailObject"]
			}],
			"targets": {
				"Start": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"controlAggregation": "masterPages",
					"viewId": "Start",
					"viewName": "Start"
				},
				"EmptyObject": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"controlAggregation": "detailPages",
					"viewName": "EmptyObject"
				},
				"DetailObject": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"controlAggregation": "detailPages",
					"viewId": "DetailObject",
					"viewName": "DetailObject"
				}
			}
		}
	}
}